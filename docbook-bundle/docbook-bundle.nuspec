    <licenseUrl>http://wiki.docbook.org/DocBookLicense</licenseUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <iconUrl>https://raw.githubusercontent.com/docbook/docbook.org/master/graphics/banner.png</iconUrl>
