<package xmlns="http://schemas.microsoft.com/packaging/2015/06/nuspec.xsd">
  <metadata>
    <!-- Read this before publishing packages to chocolatey.org: https://chocolatey.org/docs/create-packages -->
    <id>apache-fop</id>
    <title>Apache™ FOP (Install)</title>
    <version>2.6</version>
    <!-- https://xmlgraphics.apache.org/fop/team.html -->
    <authors>James Tauber, Glenn Adams, Chris Bowditch, et al</authors>
    <owners>Gerald Combs</owners>
    <summary>Apache Formatting Objects Processor</summary>
    <description>Java XSL-FO processor.
      Converts Extensible Stylesheet Language Formatting Objects (XSL-FO) to PDF,
      PS, PCL, AFP, and many other formats. Requires a Java runtime such as jre8,
      corretto8jre, or adoptopenjdk8jre.
    </description>
    <projectUrl>https://xmlgraphics.apache.org/fop/</projectUrl>
    <packageSourceUrl>https://github.com/geraldcombs/chocolatey-packages</packageSourceUrl>
    <projectSourceUrl>http://svn.apache.org/viewvc/xmlgraphics/fop/</projectSourceUrl>
    <docsUrl>https://xmlgraphics.apache.org/fop/quickstartguide.html</docsUrl>
    <mailingListUrl>https://xmlgraphics.apache.org/fop/maillist.html</mailingListUrl>
    <bugTrackerUrl>https://issues.apache.org/jira/issues/?jql=project%20%3D%20FOP</bugTrackerUrl>
    <tags>apache fop xsl docbook pdf</tags>
    <copyright>2021 The Apache Software Foundation</copyright>
    <licenseUrl>https://xmlgraphics.apache.org/fop/license.html</licenseUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <iconUrl>https://cdn.rawgit.com/geraldcombs/chocolatey-packages/master/artwork/apache-icon-128.png</iconUrl>
    <!--
      XXX Add other JREs and JVMs when https://github.com/chocolatey/choco/issues/879 is fixed?
    <dependencies>
      <dependency id="javaruntime" />
    </dependencies>
    -->
    <releaseNotes>Update to 2.6.</releaseNotes>
    <!--<provides></provides>-->
  </metadata>
  <files>
    <file src="tools\**" target="tools" />
  </files>
</package>
